{
  "expo": {
    "name": "IRM",
    "slug": "IRM",
    "scheme": "IRM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-new.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iceasanka.IRM"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.iceasanka.IRM",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "RECORD_AUDIO",
        "VIBRATE",
        "FOREGROUND_SERVICE",
        "ACCESS_NETWORK_STATE",
        "INTERNET"
      ],
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c749a7a0-ff45-4611-8bda-51bc11d52615"
      }
    },
    "owner": "iceasanka"
  }
}
